<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Zeega.Domain" namespace="Zeega.Domain">
  <class name="AppTenant" table="AppTenants" >
		<id name="Id">
			<generator class="native" />
		</id>
    <property name="name" column="Name" access="field" />
    <property name="Description" column="Description" />
    <property name="isPrimary" column="IsPrimary" access="field" />
    <component name="languageCode" access="field">
      <property name="value" column="LanguageCode" access="field" />
    </component>
	</class>
</hibernate-mapping>